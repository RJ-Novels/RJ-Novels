---
layout: null
---
<?xml version="1.0" encoding="UTF-8"?>
<rss version="2.0">
  <channel>
    <title>RJ Novels</title>
    <link>{{ site.url }}{{ site.baseurl }}/</link>
    <description>Latest novel updates from RJ Novels</description>
    <language>en-us</language>

    {% for novel in site.novels %}
    <item>
      <title>{{ novel.title | xml_escape }}</title>
      <link>{{ novel.url | absolute_url }}</link>
      <guid isPermaLink="true">{{ novel.url | absolute_url }}</guid>
      <pubDate>{{ novel.date | date_to_rfc822 }}</pubDate>
      <description>{{ novel.description | default: novel.excerpt | strip_html | xml_escape }}</description>
    </item>
    {% endfor %}

  </channel>
</rss>
